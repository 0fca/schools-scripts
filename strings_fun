#include <cstdlib>
#include <iostream>
#include <cstring>

using namespace std;

string removeSpace(string s){
    string result = "";
    for(int i = 0; i < s.size(); i++){
        char c = s.at(i);
        if(c != ' '){
            result += c;
        }
    }
    return result;
}

int main(int argc, char** argv)
{
    string s1 = "", st1 = "", st2 = "";
    getline(cin,s1);

    s1 = removeSpace(s1);

    cout<<s1<<endl;
    
    bool isOk = true;
    for(int i = 1; i <= (s1.size()/2); i++){
        st1 = s1.at(i - 1);
        st1 = tolower(st1[0]);
        st2 = s1.at(s1.size() - i);
        st2 = tolower(st2[0]);
        
        if(st1 != st2){
            cout<<"Niezgodne!"<<endl;
            isOk = false;
            break;
        }
    }

    if(isOk) cout<<"Zgodne"<<endl;
    return 0;
}
