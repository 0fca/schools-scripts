#include <iostream>
#include <cstring>
#include <stdio.h>
using namespace std;

string removeSpace(string s){
    string result = "";

    for(int i = 0; i < s.size(); i++){
        if(s.at(i) != ' '){
            result += s.at(i);
        }
    }
    return result;
}

int main()
{
    char in;
    cin>>in;
    printf("Out: %s",charToString(in));

    string s1 = "", st1 = "", st2 = "";
    getline(cin,s1);

    s1 = removeSpace(s1);

    bool isOk = true;
    for(int i = 1; i <= s1.size()/2; i++){
        st1 = s1.at(i - 1);
        st2 = s1.at(s1.size() - i );
        //cout<<st1<<":"<<st2;
        if(st1 != st2){
            cout<<"Niezgodne!"<<endl;
            isOk = false;
            break;
        }
    }

    if(isOk) cout<<"Zgodne"<<endl;
    return 0;
}
