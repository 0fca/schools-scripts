#include <iostream>
#include <cmath>

using namespace std;

struct Math{
    void fromBinToDec(int pozycja, char dana[]){
        pozycja = -1;
        int tmp = 0;
        int res = 0, res2 = 0;

        for(int i = 0; i <= sizeof(dana); i++){
            char get = dana[i];

            if((int)get == 44 | (int)get == 46){
               pozycja = i;
            }

            for(int i2 = (pozycja-1); i2 >= 0; i2--){
               int get = ((int)dana[i2] - 48);
               res += pow(2,tmp)*get;
               tmp += 1;
            }
            tmp = 0;
            for(int i3 = (pozycja+1); i3 < 10; i3++){
                int get = ((int)dana[i3] - 48);
                res2 += pow(2,tmp)*get;
                tmp -= 1;
            }
        }
        cout<<res<<"."<<res2<<endl;
    }
};


int main()
{
    Math m;
    int pozycja;
    char dana[10];
    cin>>dana;
    m.fromBinToDec(pozycja, dana);
    return 0;
}
